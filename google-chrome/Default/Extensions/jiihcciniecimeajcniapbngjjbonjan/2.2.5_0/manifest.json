{
   "background": {
      "scripts": [ "background.min.js", "heap.min.js" ]
   },
   "browser_action": {
      "default_icon": "images/icon-vidyard-128.png",
      "default_popup": "popup.html"
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "default": "Shift+Alt+V"
         }
      },
      "discard_during_recording": {
         "description": "Cancel Recording"
      },
      "pause_during_recording": {
         "description": "Pause/Resume Recording",
         "suggested_key": {
            "default": "Shift+Alt+P"
         }
      },
      "restart_during_recording": {
         "description": "Restart Recording",
         "suggested_key": {
            "default": "Shift+Alt+Z"
         }
      }
   },
   "content_scripts": [ {
      "css": [ "gmailContent.min.css" ],
      "js": [ "lib/js/inboxsdk.js", "gmailContent.min.js" ],
      "matches": [ "https://mail.google.com/*" ]
   }, {
      "js": [ "shareContent.min.js" ],
      "matches": [ "*://share.viewedit.dev/*", "*://share.viewedit.com/*", "*://share-staging.viewedit.com/*", "*://share.vidyard.dev/watch/*", "*://share.vidyard.com/watch/*", "*://share-staging.vidyard.com/watch/*" ]
   }, {
      "js": [ "welcomeContent.min.js" ],
      "matches": [ "*://welcome.viewedit.com/*", "*://welcome-staging.viewedit.com/*", "*://welcome.viewedit.dev/*", "*://welcome.vidyard.com/*", "*://welcome-staging.vidyard.com/*", "*://welcome.vidyard.dev/*" ]
   }, {
      "all_frames": true,
      "js": [ "ebertContent.min.js" ],
      "matches": [ "https://app.vidyard.com/*", "https://app-staging.vidyard.com/*", "https://app.vidyard.dev/*" ]
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval' https://faye.getstream.io https://cdn.heapanalytics.com; object-src 'self'",
   "description": "Record your screen, share your video and track who's watching it!",
   "externally_connectable": {
      "matches": [ "https://app.vidyard.com/*" ]
   },
   "icons": {
      "128": "images/icon-vidyard-128.png",
      "16": "images/icon-vidyard-16.png",
      "48": "images/icon-vidyard-48.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "49",
   "name": "Vidyard GoVideo - Screen and Webcam Recorder",
   "oauth2": {
      "client_id": "560888624396-mp0nbfg118ilj761gqf526u4rfsfa72q.apps.googleusercontent.com",
      "scopes": [ "profile", "email" ]
   },
   "permissions": [ "https://api.vidyard.com/", "identity", "identity.email", "webRequest", "webNavigation", "webRequestBlocking", "tabs", "tabCapture", "desktopCapture", "notifications", "storage", "clipboardWrite", "contextMenus", "http://*/*", "https://*/*" ],
   "short_name": "Vidyard GoVideo",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.2.5",
   "web_accessible_resources": [ "*.html", "assets/*", "images/*", "*.js", "*.css" ]
}
