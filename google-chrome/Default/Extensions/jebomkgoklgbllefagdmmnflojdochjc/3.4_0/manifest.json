{
   "background": {
      "persistent": false,
      "scripts": [ "bg.min.js" ]
   },
   "browser_action": {
      "default_icon": "images/icon.png",
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "js": [ "ee.min.js", "external/cpt.js", "external/mxp.js", "external/jquery-2.1.1.min.js", "pd.min.js", "ps.min.js" ],
      "matches": [ "https://www.irctc.co.in/*" ],
      "run_at": "document_start"
   }, {
      "js": [ "pd.min.js", "ps.min.js" ],
      "matches": [ "https://www.irctc.co.in/*" ],
      "run_at": "document_end"
   }, {
      "js": [ "ls.min.js" ],
      "matches": [ "https://www.irctc.co.in/*loginHome*", "https://www.irctc.co.in/*home" ],
      "run_at": "document_end"
   }, {
      "js": [ "jp.min.js" ],
      "matches": [ "https://www.irctc.co.in/*home", "https://www.irctc.co.in/*eticketing*" ],
      "run_at": "document_end"
   }, {
      "js": [ "tss.min.js" ],
      "matches": [ "https://www.irctc.co.in/*mainpage*", "https://www.irctc.co.in/*trainbetweenstns*" ],
      "run_at": "document_end"
   } ],
   "content_security_policy": "script-src 'self' https://cdn.mxpnl.com; object-src 'self'",
   "description": "Easy to Use ,Fully automated Extension to book IRCTC tickets ,Specially designed for TATKAL",
   "key": "",
   "manifest_version": 2,
   "name": "ZapTatkal",
   "permissions": [ "storage", "tabs" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.4",
   "web_accessible_resources": [ "/images/share/*", "/images/logo.png" ]
}
